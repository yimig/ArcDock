<Window x:Class="ArcDock.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ArcDock"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DockPanel>
            <Button DockPanel.Dock="Bottom" Height="30" Name="btnPrint" Click="BtnPrint_OnClick">打印</Button>
            <StackPanel Orientation="Vertical" Name="StMain"></StackPanel>
        </DockPanel>
    </Grid>
</Window>
